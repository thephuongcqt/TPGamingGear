<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="www.categories.vn"
           xmlns="www.categories.vn"
           xmlns:ctgr="www.category.vn"
           elementFormDefault="qualified">
    <xs:import namespace="www.category.vn" schemaLocation="Category.xsd"/>
    
    <xs:element name="Categories">       
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="ctgr:Category" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
