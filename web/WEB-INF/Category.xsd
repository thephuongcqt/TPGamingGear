<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns="www.category.vn"
           targetNamespace="www.category.vn"
           xmlns:namectgr="www.namectgr.vn"
           elementFormDefault="qualified">
    <xs:import namespace="www.namectgr.vn" schemaLocation="NameCTGR.xsd"/>    
    <xs:simpleType name="ID">
        <xs:restriction base="xs:positiveInteger">            
        </xs:restriction>
    </xs:simpleType>
    
    <xs:element name="Category">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="CategoryName" type="namectgr:Name"/>
                <xs:element name="Description" type="xs:string"/>                
            </xs:sequence>
            <xs:attribute name="CategoryId" type="ID" use="required"/>
        </xs:complexType>
    </xs:element>
</xs:schema>
